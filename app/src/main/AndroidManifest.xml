<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.duobang.dbdemo">

    <application
        >

        <activity
            android:name="com.duobang.dbdemo.ui.activity.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/FullscreenThemse">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.duobang.dbdemo.ui.activity.MainActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />

        <activity
            android:name="com.duobang.dbdemo.ui.activity.me.PersonalActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.duobang.dbdemo.ui.activity.me.NickNameActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
    </application>


</manifest>